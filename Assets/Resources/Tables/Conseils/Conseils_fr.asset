MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 0}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: e9620f8c34305754d8cc9a7e49e852d9, type: 3}
  m_Name: Conseils_fr
  m_EditorClassIdentifier: 
  m_LocaleId:
    m_Code: fr
  m_SharedData: {fileID: 11400000, guid: 37fdd8131e27346459ae0af7749749d0, type: 2}
  m_Metadata:
    m_Items: []
  m_TableData:
  - m_Id: 88742273024
    m_Localized: "Chaque Bloc est g\xE9n\xE9r\xE9 al\xE9atoirement !"
    m_Metadata:
      m_Items: []
  - m_Id: 130328797184
    m_Localized: "Plus vous irez loin, plus vous d\xE9bloquerez d'indices !"
    m_Metadata:
      m_Items: []
  - m_Id: 142748131328
    m_Localized: "Trouvez la meilleure fa\xE7on de franchir chaque obstacle !"
    m_Metadata:
      m_Items: []
  - m_Id: 163447021568
    m_Localized: "Une fois que vous aurez franchi le 10\xE8me Bloc, les Cubes commenceront
      \xE0 se d\xE9truire derri\xE8re vous !"
    m_Metadata:
      m_Items: []
  - m_Id: 172850651136
    m_Localized: "Regardez l\xE0 o\xF9 vous voulez aller plut\xF4t que l'obstacle
      que vous \xEAtes en train de franchir."
    m_Metadata:
      m_Items: []
  - m_Id: 340857692160
    m_Localized: "Les niveaux sont g\xE9n\xE9r\xE9s proc\xE9duralement."
    m_Metadata:
      m_Items: []
  - m_Id: 373816532992
    m_Localized: "Chaque fois que vous atteindrez un nouveau palier, vous d\xE9bloquerez
      un indice suppl\xE9mentaire !"
    m_Metadata:
      m_Items: []
  - m_Id: 397405298688
    m_Localized: Vous pouvez grimper dans les coins en sautant des 2 cotes !
    m_Metadata:
      m_Items: []
  - m_Id: 406607601664
    m_Localized: "Lorsque vous \xEAtes accroch\xE9 au mur, le curseur grossit et
      devient bleu."
    m_Metadata:
      m_Items: []
  - m_Id: 415084290048
    m_Localized: "Vous pouvez rep\xE9rer une Data aux reflets de sa lumi\xE8re."
    m_Metadata:
      m_Items: []
  - m_Id: 426421493760
    m_Localized: "Restez appuy\xE9 sur ESPACE pour sauter plus haut."
    m_Metadata:
      m_Items: []
  - m_Id: 714884751360
    m_Localized: Une Sonde va droit sur vous si elle vous voit.
    m_Metadata:
      m_Items: []
  - m_Id: 726037405696
    m_Localized: Sauter sur une Sonde peut vous faire aller plus haut.
    m_Metadata:
      m_Items: []
  - m_Id: 738611929088
    m_Localized: "Si une Sonde vous perd de vue, elle ira \xE0 la derni\xE8re position
      o\xF9 elle vous a vu."
    m_Metadata:
      m_Items: []
  - m_Id: 747524825088
    m_Localized: "Une Sonde qui ne vous a pas d\xE9tect\xE9 reste sur place."
    m_Metadata:
      m_Items: []
  - m_Id: 849630961664
    m_Localized: "Chaque Bloc poss\xE8de son propre poids qui le rend plus ou moins
      fr\xE9quent."
    m_Metadata:
      m_Items: []
  - m_Id: 857352675328
    m_Localized: "Lorsque vous \xEAtes accroch\xE9 au mur, les bords de votre \xE9cran
      s'assombrissent."
    m_Metadata:
      m_Items: []
  - m_Id: 863446999040
    m_Localized: "Les Cubes se d\xE9truisent de plus en plus vite."
    m_Metadata:
      m_Items: []
  - m_Id: 870749282304
    m_Localized: "N'h\xE9sitez pas \xE0 rester longtemps accroch\xE9 aux murs !"
    m_Metadata:
      m_Items: []
  - m_Id: 942505435136
    m_Localized: "Pour grimper dans un coin, vous n'avez pas besoin de vous en \xE9loigner
      \xE0 chaque saut. Restez-y coll\xE9."
    m_Metadata:
      m_Items: []
  - m_Id: 969999097856
    m_Localized: "Les niveaux sont optimis\xE9s pour limiter le nombre de culs-de-sacs
      et donc de se faire encercler par des Sondes !"
    m_Metadata:
      m_Items: []
  - m_Id: 977783726080
    m_Localized: "Les Sondes ont une port\xE9e de d\xE9tection maximum."
    m_Metadata:
      m_Items: []
  - m_Id: 984494612480
    m_Localized: "Si vous vous faites toucher par une Sonde, vous pourrez \xE0 nouveau
      vous accrocher \xE0 un mur."
    m_Metadata:
      m_Items: []
  - m_Id: 991394242560
    m_Localized: "Si tu restes trop au m\xEAme endroit, les Sondes vont s'y accumuler
      et ce sera pire !"
    m_Metadata:
      m_Items: []
  - m_Id: 1109212241920
    m_Localized: "Vous pouvez rep\xE9rer la Sortie \xE0 ses reflets lumineux plus
      intenses."
    m_Metadata:
      m_Items: []
  - m_Id: 1115134599168
    m_Localized: Les Cubes de la Mort remplissent l'espace en commencant loin de
      vous et de la Sortie.
    m_Metadata:
      m_Items: []
  - m_Id: 1121069539328
    m_Localized: "La Sortie appara\xEEt toujours \xE0 l'oppos\xE9 de votre position
      !"
    m_Metadata:
      m_Items: []
  - m_Id: 1126350168064
    m_Localized: "La Sortie bleu sera toujours la derni\xE8re Sortie."
    m_Metadata:
      m_Items: []
  - m_Id: 1132025061376
    m_Localized: "Vous pouvez vous d\xE9placer dans une direction en regardant dans
      une autre !"
    m_Metadata:
      m_Items: []
  - m_Id: 1227701329920
    m_Localized: "Appui sur SHIFT pour te d\xE9crocher des murs au bon moment et
      mieux calculer ton atterissage."
    m_Metadata:
      m_Items: []
  - m_Id: 1241399926784
    m_Localized: Appui sur SHIFT pour descendre des murs sans t'y accrocher.
    m_Metadata:
      m_Items: []
  - m_Id: 1248723181568
    m_Localized: Tu peux rester appuyer sur SHIFT pour ne jamais t'accrocher aux
      murs.
    m_Metadata:
      m_Items: []
  - m_Id: 1254771367936
    m_Localized: "Dans le doute, saute, \xE7a te laissera toujours plus de temps
      pour choisir ou aller ensuite !"
    m_Metadata:
      m_Items: []
  - m_Id: 1343761915904
    m_Localized: Vous pouvez grimper dans les coins de dos !
    m_Metadata:
      m_Items: []
  - m_Id: 1349873016832
    m_Localized: "Si vous ratez un Jump(), vous serez immobilis\xE9 et perdrez du
      temps !"
    m_Metadata:
      m_Items: []
  - m_Id: 1356445491200
    m_Localized: "Il faut \xEAtre dans les airs sans \xEAtre accroch\xE9 \xE0 un
      mur avant la fin du timer lorsque vous voyez un Jump() !\r"
    m_Metadata:
      m_Items: []
  - m_Id: 1362674032640
    m_Localized: "Vous pouvez esquiver un Jump() en appuyant sur SHIFT au bon moment
      si vous \xEAtes accroch\xE9 \xE0 un mur !"
    m_Metadata:
      m_Items: []
  - m_Id: 1369707880448
    m_Localized: Dash015 permet de dasher en avant toutes les 15 secondes avec le
      clic gauche !
    m_Metadata:
      m_Items: []
  - m_Id: 1462880149504
    m_Localized: La prochaine Data accessible sera la plus proche de la Data d'avant
      !
    m_Metadata:
      m_Items: []
  - m_Id: 1486720573440
    m_Localized: "Courir le long des murs est souvent une bonne id\xE9e pour gagner
      de la hauteur."
    m_Metadata:
      m_Items: []
  - m_Id: 1492676485120
    m_Localized: "Sautez contre les parois du niveau pour vous \xE9lever rapidement."
    m_Metadata:
      m_Items: []
  - m_Id: 1498590453760
    m_Localized: "Les Datas transparentes ne peuvent pas \xEAtre captur\xE9es. Pour
      le moment."
    m_Metadata:
      m_Items: []
  - m_Id: 1504722526208
    m_Localized: Monter par les coins c'est bien, mais ce n'est souvent pas le plus
      rapide.
    m_Metadata:
      m_Items: []
  - m_Id: 1512251301888
    m_Localized: La prochaine Data peut se trouver juste au-dessus ou en-dessous
      de vous !
    m_Metadata:
      m_Items: []
  - m_Id: 1528038662144
    m_Localized: "Essayez d'anticiper quelle sera la prochaine Data \xE0 l'avance
      pour gagner du temps !"
    m_Metadata:
      m_Items: []
  - m_Id: 1685606080512
    m_Localized: "Essayez de toucher le moins de Cubes possible pour les conserver
      pour apr\xE8s."
    m_Metadata:
      m_Items: []
  - m_Id: 1691767513088
    m_Localized: Accrochez-vous au bord du Cube avant de sauter dessus s'il est trop
      haut.
    m_Metadata:
      m_Items: []
  - m_Id: 1698344181760
    m_Localized: "Les lignes sont toujours espac\xE9s verticalement de 2 Cubes, donc
      vous pouvez toujours sauter de l'une \xE0 l'autre."
    m_Metadata:
      m_Items: []
  - m_Id: 1705738739712
    m_Localized: "Les Cubes mettent 2 secondes \xE0 se d\xE9truire, vous pouvez encore
      les utiliser pendant ce temps l\xE0 !"
    m_Metadata:
      m_Items: []
  - m_Id: 1713477230592
    m_Localized: "Restez appuy\xE9 jusqu'au bo\xFBt sur ESPACE pour faire de grands
      sauts !"
    m_Metadata:
      m_Items: []
  - m_Id: 1735077896192
    m_Localized: "Prenez le temps de regardez l\xE0 o\xF9 vous aller arriver avant
      de sauter."
    m_Metadata:
      m_Items: []
  - m_Id: 1860177207296
    m_Localized: Elle fuit votre regard !
    m_Metadata:
      m_Items: []
  - m_Id: 1866862927872
    m_Localized: "Coincez-l\xE0 sur les bords avant de vous en rapprocher !"
    m_Metadata:
      m_Items: []
  - m_Id: 1872500072448
    m_Localized: "Vous pouvez la faire venir \xE0 vous, en ne bougeant presque pas
      !"
    m_Metadata:
      m_Items: []
  - m_Id: 1877822644224
    m_Localized: Plus vous la fixez, plus elle ira vite.
    m_Metadata:
      m_Items: []
  - m_Id: 1884395118592
    m_Localized: "Vous pouvez regarder dans une direction et vous d\xE9placer dans
      une autre !"
    m_Metadata:
      m_Items: []
  - m_Id: 2003823730688
    m_Localized: "Les Cubes sont d\xE9truits rapidement au d\xE9but et lentement
      \xE0 la fin."
    m_Metadata:
      m_Items: []
  - m_Id: 2010639474688
    m_Localized: Prenez de la hauteur, vous tomberez moins facilement.
    m_Metadata:
      m_Items: []
  - m_Id: 2016591192064
    m_Localized: "Les Sorties apparaissent toujours \xE0 l'oppos\xE9 de votre position
      !"
    m_Metadata:
      m_Items: []
  - m_Id: 2022333194240
    m_Localized: "Les Cubes proches de vous et de la Sortie sont d\xE9truits en derniers."
    m_Metadata:
      m_Items: []
  - m_Id: 2028494626816
    m_Localized: "Prenez le temps de savoir o\xF9 se trouve la Sortie avancer de
      foncer nulle part."
    m_Metadata:
      m_Items: []
  - m_Id: 2035390062592
    m_Localized: "Sautez en permanence pour \xE9viter de tomber par inadvertance."
    m_Metadata:
      m_Items: []
  - m_Id: 2150448209920
    m_Localized: "Attirez les Sondes \xE0 un autre endroit si elles s'accumulent
      !"
    m_Metadata:
      m_Items: []
  - m_Id: 2156504784896
    m_Localized: "Vous pouvez voir o\xF9 se trouvent les Datas \xE0 travers les murs."
    m_Metadata:
      m_Items: []
  - m_Id: 2162313895936
    m_Localized: Prenez du recul pour mieux visualiser le niveau.
    m_Metadata:
      m_Items: []
  - m_Id: 2168496300032
    m_Localized: "Essayer de pr\xE9voir \xE0 l'avance la prochaine Data qui sera
      capturable."
    m_Metadata:
      m_Items: []
  - m_Id: 2174502543360
    m_Localized: Essayez de toujours prendre de la hauteur.
    m_Metadata:
      m_Items: []
  - m_Id: 2180605255680
    m_Localized: Vous pouvez anticiper le mouvement des Sondes si vous les voyez
      de loin.
    m_Metadata:
      m_Items: []
  - m_Id: 2276394770432
    m_Localized: Les Intersections() vous visent depuis les 6 directions !
    m_Metadata:
      m_Items: []
  - m_Id: 2292958076928
    m_Localized: "Les Intersections() mettent 1.5 secondes avant de se d\xE9clencher
      !"
    m_Metadata:
      m_Items: []
  - m_Id: 2298565861376
    m_Localized: Les Intersections() se referment toujours en 1 seconde. Donc si
      elle part de loin, elle arrivera vite !
    m_Metadata:
      m_Items: []
  - m_Id: 2304458858496
    m_Localized: "D\xE9placez-vous au maximum en diagonales pour \xE9viter les Intersection()
      facilement !"
    m_Metadata:
      m_Items: []
  - m_Id: 2310901309440
    m_Localized: "Vous pouvez savoir o\xF9 vise une Intersection() aux symboles de
      pr\xE9visualisation."
    m_Metadata:
      m_Items: []
  - m_Id: 2403389906944
    m_Localized: Si vous ne la voyez pas, elle ne vous fuit pas.
    m_Metadata:
      m_Items: []
  - m_Id: 2413435265024
    m_Localized: "Vous ne l'effrayez pas si vous la regardez \xE0 travers le verre."
    m_Metadata:
      m_Items: []
  - m_Id: 2418594258944
    m_Localized: "Prevoyez votre trajectoire \xE0 l'avance, regardez ailleurs, puis
      allez-y !"
    m_Metadata:
      m_Items: []
  - m_Id: 2425707798528
    m_Localized: Approcher par en haut est une technique efficace.
    m_Metadata:
      m_Items: []
  - m_Id: 2435451166720
    m_Localized: "Elle ne d\xE9tecte pas votre regard \xE0 travers le verre."
    m_Metadata:
      m_Items: []
  - m_Id: 2532045987840
    m_Localized: "Les niveaux sont optimis\xE9s pour que chaque chemin ait toujours
      un d\xE9but et une fin : vous pouvez rentrer par n'importe o\xF9, vous avez
      la certitude de pouvoir ressortir de l'autre c\xF4t\xE9 !"
    m_Metadata:
      m_Items: []
  - m_Id: 2540514287616
    m_Localized: "Utiliser PathFinder() ne vous co\xFBte qu'une seconde."
    m_Metadata:
      m_Items: []
  - m_Id: 2547288088576
    m_Localized: PathFinder() a 10 secondes de cooldown, utilisez-le au bon moment.
    m_Metadata:
      m_Items: []
  - m_Id: 2553357246464
    m_Localized: "PathFinder() vous montre le chemin \xE0 la Data la plus proche."
    m_Metadata:
      m_Items: []
  - m_Id: 2559531261952
    m_Localized: Le PathFinder() peut partir dans votre dos, regardez bien autour
      de vous !
    m_Metadata:
      m_Items: []
  - m_Id: 2566305062912
    m_Localized: Le PathFinder() ne vous permet pas de localiser la Sortie.
    m_Metadata:
      m_Items: []
  - m_Id: 2577789067264
    m_Localized: "La prochaine Data accessible sera la plus \xE9loign\xE9e de la
      Data d'avant !"
    m_Metadata:
      m_Items: []
  - m_Id: 1328368984498176
    m_Localized: Dash015 a un long cooldown d'utilisation, utilisez-le au bon moment
      !
    m_Metadata:
      m_Items: []
  - m_Id: 1328613977989120
    m_Localized: "Dash015 permet de vous accrocher \xE0 nouveau \xE0 un mur apr\xE8s
      une utilisation !"
    m_Metadata:
      m_Items: []
  - m_Id: 1328863073509376
    m_Localized: "Dash015 permet \xE9galement de dasher vers le haut ou le bas !"
    m_Metadata:
      m_Items: []
  - m_Id: 8726365015875584
    m_Localized: "Un Bouncer vous pousse de 3 cubes dans la direction d'o\xF9 vous
      venez."
    m_Metadata:
      m_Items: []
  - m_Id: 9343970839863296
    m_Localized: "Lorsque vous prenez un Bouncer, vous pourrez \xE0 nouveau vous
      accrochez \xE0 n'importe quel mur."
    m_Metadata:
      m_Items: []
  - m_Id: 9344550270377984
    m_Localized: "Lorsqu'un Bouncer vous pousse vers le haut, vous avez le choix
      entre vous laisser pousser ou sauter en m\xEAme temps pour aller plus haut."
    m_Metadata:
      m_Items: []
  - m_Id: 9345456923713536
    m_Localized: Pour sauter sur un Bouncer, maintenez ESPACE avant l'impact avec
      le Bouncer.
    m_Metadata:
      m_Items: []
  - m_Id: 12998157704339456
    m_Localized: Ici les Sorties alternent entre le haut et le bas.
    m_Metadata:
      m_Items: []
  - m_Id: 14132302543298560
    m_Localized: "L'espace au-dessus d'un Bouncer est presque toujours d\xE9gag\xE9."
    m_Metadata:
      m_Items: []
  - m_Id: 14132631620001792
    m_Localized: "Les Bouncers sont align\xE9s verticalement \xE0 un cube pr\xE8s
      pour que vous puissiez les enchainer !"
    m_Metadata:
      m_Items: []
  - m_Id: 14132631838105600
    m_Localized: Regardez vers le bas lorsque vous enchainez les Bouncers pour mieux
      les voirs.
    m_Metadata:
      m_Items: []
  - m_Id: 14137308692586496
    m_Localized: "Attraper un Reset Temporel remonte votre temps \xE0 son maximum
      !"
    m_Metadata:
      m_Items: []
  - m_Id: 16568985448587264
    m_Localized: "Les Tracers traceront \xE0 la premi\xE8re position o\xF9 ils vous
      ont vu, avant de charger un Blast en 4 secondes."
    m_Metadata:
      m_Items: []
  - m_Id: 16568935427317760
    m_Localized: "Le Pathfinder vous permet de localiser \xE0 la fois une Data en
      jaune et un ResetTemporel en violet !"
    m_Metadata:
      m_Items: []
  - m_Id: 16568946156347392
    m_Localized: "Chaque fois que vous attraperez un Reset Temporel, un autre appara\xEEtra
      ailleurs !"
    m_Metadata:
      m_Items: []
  - m_Id: 16568956679856128
    m_Localized: Utilisez votre Pathfinder pour localiser un Reset Temporel lorsque
      manquez de temps !
    m_Metadata:
      m_Items: []
  - m_Id: 16568965601140736
    m_Localized: "Si une Sonde vous \xE9jecte hors de la Matrice, utilisez votre
      Dash015 pour revenir !"
    m_Metadata:
      m_Items: []
  - m_Id: 18479190348374016
    m_Localized: "Le Blast vous touchent quelque soit la distance si vous \xEAtes
      en ligne de vue du Tracer."
    m_Metadata:
      m_Items: []
  - m_Id: 18479202167922688
    m_Localized: Si vous ne voyez pas le centre du Tracer au moment du Blast alors
      celui-ci ne vous touchera pas !
    m_Metadata:
      m_Items: []
  - m_Id: 18479225588916224
    m_Localized: Vous pouvez interrompre un Tracer pendant 6 secondes en lui sautant
      dessus !
    m_Metadata:
      m_Items: []
  - m_Id: 18479234145296384
    m_Localized: "Il vaut parfois mieux attendre cach\xE9 que vos cooldown remontent
      et que les Tracers se soient calm\xE9s avant de repartir !"
    m_Metadata:
      m_Items: []
  - m_Id: 19824135923539968
    m_Localized: Dash15 permet de dasher en avant toutes les 1.5 secondes avec le
      clic gauche !
    m_Metadata:
      m_Items: []
  - m_Id: 20165620455940096
    m_Localized: "Dash15 permet de vous accrocher \xE0 nouveau \xE0 un mur apr\xE8s
      une utilisation !"
    m_Metadata:
      m_Items: []
  - m_Id: 20165651657367552
    m_Localized: "Dash15 permet \xE9galement de dasher vers le haut ou le bas !"
    m_Metadata:
      m_Items: []
  - m_Id: 20165664051535872
    m_Localized: "Ceci est un niveau d'entra\xEEnement, par la suite vous utiliserez
      Dash<b>0</b>15 qui aura un cooldown de 15 secondes."
    m_Metadata:
      m_Items: []
  - m_Id: 26060389874900992
    m_Localized: "Les Cubes transparents sont d\xE9sactiv\xE9s et ne fonctionnent
      plus. Vous pouvez passer \xE0 travers."
    m_Metadata:
      m_Items: []
  - m_Id: 26060467297558528
    m_Localized: "Les LinkyCubes sont des cubes connect\xE9s les uns aux autres.
      Ce qui arrive \xE0 l'un arrive \xE0 tous ceux connect\xE9s."
    m_Metadata:
      m_Items: []
  - m_Id: 26060477468745728
    m_Localized: "Les SwappyCubes sont des LinkyCubes qui s'activent et se d\xE9sactivent
      cycliquement en rythme."
    m_Metadata:
      m_Items: []
  - m_Id: 26060486461333504
    m_Localized: "Un cercle de pr\xE9visualisation vous permet d'anticiper quand
      des SwappyCubes changent d'\xE9tat."
    m_Metadata:
      m_Items: []
  - m_Id: 26060495361646592
    m_Localized: "Les poign\xE9es rondes sur les bords des LinkyCubes vous indique
      que ce sont des SwappyCubes."
    m_Metadata:
      m_Items: []
  references:
    version: 1
